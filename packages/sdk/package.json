{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/xl1-sdk",
  "version": "1.22.29",
  "description": "XYO Layer One Protocol",
  "homepage": "https://xylabs.com",
  "bugs": {
    "url": "git+https://github.com/xylabs/xyo-chain/issues",
    "email": "support@xylabs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xylabs/xyo-chain.git"
  },
  "license": "LGPL-3.0-only",
  "author": {
    "name": "XY Labs Development Team",
    "email": "support@xylabs.com",
    "url": "https://xylabs.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/neutral/index.d.ts",
      "source": "./src/index.ts",
      "default": "./dist/neutral/index.mjs"
    },
    "./test": {
      "types": "./dist/neutral/test/index.d.ts",
      "source": "./src/test/index.ts",
      "default": "./dist/neutral/test/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/neutral/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/neutral/index.d.ts",
  "files": [
    "dist",
    "src",
    "!**/*.bench.*",
    "!**/*.spec.*",
    "!**/*.test.*"
  ],
  "workspaces": [
    "packages/*",
    "packages/*/packages/*",
    "packages/*/packages/*/packages/*"
  ],
  "dependencies": {
    "@xyo-network/xl1-network-model": "~1.22.29",
    "@xyo-network/xl1-protocol": "~1.22.29",
    "@xyo-network/xl1-protocol-sdk": "~1.22.29",
    "@xyo-network/xl1-providers": "~1.22.29",
    "@xyo-network/xl1-rpc": "~1.22.29",
    "@xyo-network/xl1-schema": "~1.22.29",
    "@xyo-network/xl1-validation": "~1.22.29",
    "@xyo-network/xl1-wrappers": "~1.22.29"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "~7.3.2",
    "@xylabs/tsconfig": "~7.3.2",
    "axios": "^1.13.4",
    "typescript": "~5.9.3",
    "zod": "~4.3.6"
  },
  "peerDependencies": {
    "axios": "^1",
    "zod": "^4"
  },
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": ">=22.3 <23"
  },
  "engineStrict": true,
  "volta": {
    "node": "22.3.0",
    "yarn": "4.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
